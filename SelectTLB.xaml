<Window x:Class="eOPS.SelectTLB"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:eOPS"
        mc:Ignorable="d"
        Title="SelectTLB" Height="450" Width="310.479">
    <Grid>
        <TextBox x:Name="TBreg" HorizontalAlignment="Left" Height="21" Margin="71,106,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="142" UndoLimit="2" TextChanged="TBreg_TextChanged"/>
        <Label Content="REG:" HorizontalAlignment="Left" Height="30" Margin="31,105,0,0" VerticalAlignment="Top" Width="48"/>
        <CheckBox Content="Maintenance" HorizontalAlignment="Left" Margin="70,177,0,0" VerticalAlignment="Top"/>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Height="17" Margin="70,157,0,0" VerticalAlignment="Top" Width="97"/>
        <Button x:Name="TlbOpen" Content="OPEN" HorizontalAlignment="Left" Height="25" Margin="31,226,0,0" VerticalAlignment="Top" Width="73" Click="TlbOpen_Click"/>
        <Button x:Name="BtnCancel" Content="Cancel" HorizontalAlignment="Left" Height="25" Margin="195,226,0,0" VerticalAlignment="Top" Width="73" Click="BtnCancel_Click"/>

    </Grid>
</Window>
